{
  "name": "dataloaderx",
  "version": "2.1.1",
  "description": "DataLoaderX is an expansion upon DataLoader, designed to abstract some details away, and add common functionality",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.build-cjs.json",
    "build:esm": "tsc --project tsconfig.build-esm.json",
    "build:watch": "tsc --watch",
    "lint:code": "eslint src",
    "lint:style": "prettier --check src",
    "release": "semantic-release",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:mutation": "stryker run"
  },
  "keywords": [
    "dataloader",
    "graphql",
    "cache",
    "caching",
    "batch",
    "batching"
  ],
  "author": {
    "name": "Ben Ezard",
    "email": "ezard.ben@gmail.com",
    "url": "ezard.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ezard/dataloaderx"
  },
  "license": "MIT",
  "dependencies": {
    "dataloader": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.38.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/npm": "12.0.2",
    "@stryker-mutator/core": "8.7.1",
    "@stryker-mutator/jest-runner": "8.7.1",
    "@stryker-mutator/typescript-checker": "8.7.1",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "eslint": "9.38.0",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-import": "2.32.0",
    "globals": "15.15.0",
    "jest": "29.7.0",
    "prettier": "3.6.2",
    "semantic-release": "24.2.9",
    "ts-jest": "29.4.5",
    "typescript": "5.9.3"
  }
}
