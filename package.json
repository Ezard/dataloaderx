{
  "name": "dataloaderx",
  "version": "1.2.1",
  "description": "DataLoaderX is an expansion upon DataLoader, designed to abstract some details away, and add common functionality",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.build-cjs.json",
    "build:esm": "tsc --project tsconfig.build-esm.json",
    "build:watch": "tsc --watch",
    "lint:code": "eslint src",
    "lint:style": "prettier --check src",
    "release": "semantic-release",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:mutation": "stryker run"
  },
  "keywords": [
    "dataloader",
    "graphql",
    "cache",
    "caching",
    "batch",
    "batching"
  ],
  "author": {
    "name": "Ben Ezard",
    "email": "ezard.ben@gmail.com",
    "url": "ezard.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ezard/dataloaderx"
  },
  "license": "MIT",
  "dependencies": {
    "dataloader": "^2.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.4",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.4",
    "@semantic-release/npm": "10.0.6",
    "@stryker-mutator/core": "6.4.2",
    "@stryker-mutator/jest-runner": "6.4.2",
    "@stryker-mutator/typescript-checker": "6.4.2",
    "@types/jest": "29.5.10",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "jest": "29.7.0",
    "prettier": "3.1.0",
    "semantic-release": "21.1.2",
    "ts-jest": "29.1.1",
    "typescript": "4.8.4"
  }
}
