{
  "name": "dataloaderx",
  "version": "1.2.1",
  "description": "DataLoaderX is an expansion upon DataLoader, designed to abstract some details away, and add common functionality",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.build-cjs.json",
    "build:esm": "tsc --project tsconfig.build-esm.json",
    "build:watch": "tsc --watch",
    "lint:code": "eslint src",
    "lint:style": "prettier --check src",
    "release": "semantic-release",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:mutation": "stryker run"
  },
  "keywords": [
    "dataloader",
    "graphql",
    "cache",
    "caching",
    "batch",
    "batching"
  ],
  "author": {
    "name": "Ben Ezard",
    "email": "ezard.ben@gmail.com",
    "url": "ezard.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ezard/dataloaderx"
  },
  "license": "MIT",
  "dependencies": {
    "dataloader": "^2.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "9.0.1",
    "@stryker-mutator/core": "5.6.1",
    "@stryker-mutator/jest-runner": "5.6.1",
    "@stryker-mutator/typescript-checker": "5.6.1",
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "semantic-release": "19.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  }
}
