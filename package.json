{
  "name": "dataloaderx",
  "version": "1.2.1",
  "description": "DataLoaderX is an expansion upon DataLoader, designed to abstract some details away, and add common functionality",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.build-cjs.json",
    "build:esm": "tsc --project tsconfig.build-esm.json",
    "build:watch": "tsc --watch",
    "lint:code": "eslint src",
    "lint:style": "prettier --check src",
    "release": "semantic-release",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:mutation": "stryker run"
  },
  "keywords": [
    "dataloader",
    "graphql",
    "cache",
    "caching",
    "batch",
    "batching"
  ],
  "author": {
    "name": "Ben Ezard",
    "email": "ezard.ben@gmail.com",
    "url": "ezard.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ezard/dataloaderx"
  },
  "license": "MIT",
  "dependencies": {
    "dataloader": "^2.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@stryker-mutator/core": "5.3.0",
    "@stryker-mutator/jest-runner": "5.3.0",
    "@stryker-mutator/typescript-checker": "5.3.0",
    "@types/jest": "27.0.1",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.1",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "semantic-release": "17.4.5",
    "ts-jest": "27.0.5",
    "typescript": "4.3.5"
  }
}
